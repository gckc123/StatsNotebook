{
  "name": "StatsNotebook",
  "version": "0.1.4",
  "private": true,
  "main": "./public/electron.js",
  "homepage": "./",
  "build": {
    "productName": "StatsNotebook",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extraResources": [
      {
        "from": "./extraResources/",
        "to": "extraResources",
        "filter": [
          "**/*"
        ]
      },
      "public/electron.js"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "ace-builds": "^1.4.11",
    "bootstrap": "^4.4.1",
    "draft-js": "^0.11.5",
    "electron-is-dev": "^1.2.0",
    "lodash": "^4.17.15",
    "node-gyp": "^6.1.0",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-ace": "^8.1.0",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-grid-layout": "^0.18.3",
    "react-hotkeys": "^2.0.0",
    "react-quill": "^1.3.5",
    "react-resize-detector": "^4.2.3",
    "react-scripts": "5.0.1",
    "react-simple-code-editor": "^0.11.0",
    "react-virtualized": "^9.21.2",
    "rescripts": "0.0.1-security",
    "typeface-roboto": "0.0.75",
    "zeromq": "^5.2.8"
  },
  "scripts": {
    "electron": "electron .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-electron": "mkdir build/extraResources && robocopy electron build/electron /S & robocopy extraResources build/extraResources /S",
    "package:win": "electron-builder build --win -c.extraMetadata.main=build/electron.js",
    "package:mac": "electron-builder build -m -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "devDependencies": {
    "electron": "^19.0.9",
    "electron-builder": "^23.3.3"
  }
}
